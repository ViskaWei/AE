{
    "data": {
      "dir":"./data",
      "filename": "pca.h5",
      "train": "flux_std",
      "norm": "no"
    },
    "model":{
      "aug": true,
      "name": "t_act_",
      "input_dim": 300,
      "latent_dim": 300,
      "hidden_dims": [],
      "dropout": 0.0,
      "lr": 0.001,
      "reg1": 0.1,
      "loss": "mae",
      "opt": "adam",
      "batchnorm": false,
      "act_in": "tanh",
      "act_em": "linear",
      "act_hd": "linear"
    },
    "trainer":{
      "epoch": 100,
      "batch_size": 64,
      "validation_split":0.2,
      "verbose": 0
    },
    "callbacks":{
        "checkpoint_monitor": "val_loss",
        "checkpoint_mode": "min"
    }
}