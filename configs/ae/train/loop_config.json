{
  
  "data": {
    "dir":"./data",
    "filename": "pc32.h5",
    "train": "flux32ms"
  },
  "model":{
    "aug": 1,
    "name": "pc32ms",
    "input_dim": 32,
    "latent_dim": 5,
    "hidden_dims": [128, 64],
    "dropout": 0.001,
    "lr": 0.003,
    "reg1": 0.00001,
    "loss": "mse",
    "opt": "adam",
    "batchnorm": 0,
    "act_in": "linear",
    "act_em": "leaky",
    "act_hd": "leaky"
  },
  "trainer":{
    "epoch": 300,
    "batch_size": 16,
    "validation_split":0.3,
    "verbose": 0,
    "save": 0
  },
  "callbacks":{
      "checkpoint_monitor": "val_loss",
      "checkpoint_mode": "min"
  }
}