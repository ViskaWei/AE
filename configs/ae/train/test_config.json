{
  
    "data": {
      "dir":"./data",
      "filename": "pc32.h5",
      "train": "flux32ms"
    },
    "model":{
      "aug": 1,
      "name": "pc32ms",
      "input_dim": 32,
      "latent_dim": 8,
      "hidden_dims": [512, 128],
      "dropout": 0.0,
      "lr": 0.001,
      "reg1": 0.00001,
      "loss": "mse",
      "opt": "sgd",
      "batchnorm": 0,
      "act_in": "linear",
      "act_em": "leaky",
      "act_hd": "leaky"
    },
    "trainer":{
      "epoch": 500,
      "batch_size": 16,
      "validation_split":0.3,
      "verbose": 1,
      "save": 1
    },
    "callbacks":{
        "checkpoint_monitor": "val_loss",
        "checkpoint_mode": "min"
    }
}